{
  "name": "app/php",
  "description": "PHP_APP_WITH_NIX",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-apcu": "*",
    "ext-bcmath": "*",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-fileinfo": "*",
    "ext-ftp": "*",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pcre": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-posix": "*",
    "ext-redis": "*",
    "ext-session": "*",
    "ext-simplexml": "*",
    "ext-sodium": "*",
    "ext-soap": "*",
    "ext-sqlite3": "*",
    "ext-zend-opcache": "*",
    "ext-zip": "*",
    "ext-zlib": "*",
    "mailgun/mailgun-php": "dev-master as v3.5.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "continued-llc/php-vcr": "dev-master",
    "continued-llc/phpunit-testlistener-vcr": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Continued-llc/php-vcr"
    },
    {
      "type": "vcs",
      "url": "https://github.com/Continued-llc/phpunit-testlistener-vcr"
    }
  ],
  "minimum-stability": "stable"
}
